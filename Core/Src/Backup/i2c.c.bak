/**
  * @brief app_I2C_stop
  * @param  I2C_HandleTypeDef *hi2c
  *         
  * @note   app_I2C_stop  
  * @retval None
  */
 void app_I2C_stop(I2C_HandleTypeDef *hi2c)
{  
		__HAL_I2C_DISABLE(hi2c);	//stop	
}
/**
  * @brief app_I2C_start
  * @param  I2C_HandleTypeDef *hi2c
  *         
  * @note   app_I2C_start  
  * @retval None
  */
 void app_I2C_start(I2C_HandleTypeDef *hi2c)
{  
		__HAL_I2C_ENABLE(hi2c);	//start	
}

/**
  * @brief app_I2C_Error_handle
  * @param  I2C_HandleTypeDef *hi2c
  *         
  * @note   app_I2C_Error_handle  
  * @retval None
  */
 void app_I2C_Error_handle(I2C_HandleTypeDef *hi2c)
{  
		switch(hi2c->ErrorCode)
		{
			case HAL_OK:
				break;
			case HAL_ERROR:
				HAL_I2C_MspDeInit(hi2c);
				HAL_I2C_MspInit(hi2c);//重启
				break;
			case HAL_BUSY:
				__HAL_I2C_DISABLE(hi2c);	//stop
			HAL_Delay(1);
				__HAL_I2C_ENABLE(hi2c);	//stop
				break;
			case HAL_TIMEOUT:
				__HAL_I2C_DISABLE(hi2c);	//stop
				HAL_Delay(1);
				__HAL_I2C_ENABLE(hi2c);	//stop
				break;
			default:
				break;			
		}
		
}
/**
  * @brief HAL_I2C_ErrorCallback
  * @param  I2C_HandleTypeDef *hi2c
  *         
  * @note   HAL_I2C_ErrorCallback ，stop IIC
  * @retval None
  */
 void HAL_I2C_ErrorCallback(I2C_HandleTypeDef *hi2c)
{  
  //UNUSED(hi2c);
	//stop IIC(); //send stop  
		__HAL_I2C_DISABLE(hi2c);	//stop
		HAL_I2C_MspDeInit(hi2c);//restart
		HAL_I2C_DeInit(hi2c);
		__HAL_I2C_ENABLE(hi2c);
	//send rtos error 
}
